require 'puppetlabs_spec_helper/rake_tasks'
require 'puppet-syntax/tasks/puppet-syntax'
<% if ! @configs['requires'].nil? -%>
<%   @configs['requires'].each do |item| -%>
require '<%= item %>'
<%   end -%>
<% end -%>
<% if ! @configs['imports'].nil? %>
<%   @configs['imports'].each do |item| -%>
import '<%= item %>'
<%   end -%>
<% end -%>
